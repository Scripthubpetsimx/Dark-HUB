-- Venus UI Library (Safe Alternative)
local Venus = loadstring(game:HttpGet("https://raw.githubusercontent.com/weakhoes/Roblox-UI-Libs/refs/heads/main/Venus%20Lib/Venus%20Lib%20Source.lua"))()

-- Create window
local Window = Venus:CreateWindow({
    Title = "EXECUTIVERAT IN THE SEWER",
    Center = true,
    AutoShow = true,
    TabWidth = 160,
    Size = UDim2.fromOffset(600, 400),
    Theme = {
        Background = Color3.fromRGB(25, 25, 25),
        Glow = Color3.fromRGB(235, 69, 69),
        Accent = Color3.fromRGB(9, 149, 98),
        LightContrast = Color3.fromRGB(35, 35, 35),
        DarkContrast = Color3.fromRGB(20, 20, 20),
        TextColor = Color3.fromRGB(240, 240, 240)
    }
})

-- Create tabs
local LoaderTab = Window:AddTab("Loader", "rbxassetid://7072722619")
local DupeTab = Window:AddTab("DUPE/VULN", "rbxassetid://7072708282")
local AnimTab = Window:AddTab("Animations", "rbxassetid://7072723980")

-- Initial notification
Venus:Notify({
    Title = "EXECUTIVERAT",
    Text = "Snatched Scripts - Summer Event Script 9999+ points",
    Duration = 5
})

-- Tab 1: Loader
LoaderTab:AddButton({
    Title = "No-Lag OFFICIAL v1",
    Description = "Official No-Lag Hub Loader v1",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/main/Loader/LoaderV1.lua"))()
    end
})

LoaderTab:AddButton({
    Title = "No-Lag OFFICIAL v2",
    Description = "Official No-Lag Hub Loader v2",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/main/Loader/LoaderV2.lua"))()
    end
})

LoaderTab:AddButton({
    Title = "No-Lag OFFICIAL v3",
    Description = "Official No-Lag Hub Loader v3",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/main/Loader/LoaderV3.lua"))()
    end
})

LoaderTab:AddButton({
    Title = "Aussie-WIRE",
    Description = "Grow a Garden exploit",
    Callback = function()
        loadstring(game:HttpGet("https://scriptblox.com/raw/Grow-a-Garden-Aussie-WIRE-vs-GrowAGarden-42380"))()
    end
})

-- Tab 2: DUPE/VULN Key System
local keyInput = ""
local keyVerified = false
local correctKey = "EXECUTIVERAT_FREE_aMB0Ts12s4gjaivmr_var111113321321123"

-- Key UI elements
DupeTab:AddButton({
    Title = "Copy Key Link",
    Description = "Get your access key",
    Callback = function()
        setclipboard("https://link-center.net/431614/EQVrnV89bzeb")
        Venus:Notify({
            Title = "COPIED",
            Text = "Key link copied to clipboard!",
            Duration = 3
        })
    end
})

local keyTextBox = DupeTab:AddTextBox({
    Title = "Enter Key",
    Default = "",
    Placeholder = "Paste key here",
    Callback = function(Value)
        keyInput = Value
    end
})

local submitBtn = DupeTab:AddButton({
    Title = "Submit Key",
    Description = "Verify your access",
    Callback = function()
        if keyInput == correctKey then
            keyVerified = true
            Venus:Notify({
                Title = "SUCCESS",
                Text = "Key accepted! Loading DUPE/VULN features...",
                Duration = 3
            })
            LoadDupeFeatures()
        else
            Venus:Notify({
                Title = "ERROR",
                Text = "Invalid key!",
                Duration = 3
            })
        end
    end
})

-- Function to create DUPE features (initially hidden)
local dupeSection
function CreateDupeFeatures()
    dupeSection = DupeTab:AddSection("DUPE/VULN Features")
    
    -- VULN Features
    dupeSection:AddLabel("Skip Craft Time (PATCHED)")
    dupeSection:AddLabel("Skip Seed Time (PATCHED)")
    dupeSection:AddButton({Title = "Egg ESP"})
    
    -- Egg Finder
    dupeSection:AddDropdown({
        Title = "Egg Finder",
        Options = {
            "Raccoon (Night Egg)", 
            "Disco Bee (Anti Bee Egg)", 
            "Butterfly (Anti Bee Egg)", 
            "Queen Bee (Bee Egg)", 
            "Red Fox (Mythical Egg)", 
            "Mimic Octopus (Paradise Egg)"
        },
        Callback = function(Value) 
            Venus:Notify({
                Title = "Egg Selected",
                Text = "Finding: "..Value,
                Duration = 3
            })
        end
    })
    
    dupeSection:AddButton({Title = "Egg Hop"})
    
    -- DUPE Features
    dupeSection:AddLabel("DUPE -")
    
    -- Pet Selection
    local selectedPet = ""
    dupeSection:AddDropdown({
        Title = "Select Pet",
        Options = {
            "Raccoon", 
            "Disco Bee", 
            "Butterfly", 
            "Queen Bee", 
            "Red Fox", 
            "Mimic Octopus"
        },
        Callback = function(Value) 
            selectedPet = Value
        end
    })
    
    -- Size and Age
    local petSize, petAge = 1, 1
    dupeSection:AddTextBox({
        Title = "Size (Number)",
        Default = "1",
        Callback = function(Value) 
            petSize = tonumber(Value) or 1
        end
    })
    
    dupeSection:AddTextBox({
        Title = "Age (Number)",
        Default = "1",
        Callback = function(Value) 
            petAge = tonumber(Value) or 1
        end
    })
    
    dupeSection:AddButton({
        Title = "Execute Dupe",
        Callback = function()
            Venus:Notify({
                Title = "DUPE STARTED",
                Text = string.format("Duping %s (Size: %d, Age: %d)", selectedPet, petSize, petAge),
                Duration = 5
            })
        end
    })
    
    dupeSection:AddLabel("Server Dupe Compatibility: Not Compatible")
    
    -- Steal Fruits
    dupeSection:AddLabel("Steal Others Fruit")
    local stealInput = dupeSection:AddTextBox({
        Title = "Username",
        Default = "",
        Callback = function() end
    })
    
    dupeSection:AddButton({
        Title = "Whitelist User",
        Callback = function() 
            Venus:Notify({
                Title = "WHITELISTED",
                Text = "Added user to whitelist",
                Duration = 3
            })
        end
    })
    
    -- Shovel Section
    dupeSection:AddLabel("Shovel Other Garden Fruits")
    local shovelInput = dupeSection:AddTextBox({
        Title = "Username",
        Default = "",
        Callback = function() end
    })
    
    dupeSection:AddButton({
        Title = "Whitelist User",
        Callback = function() 
            Venus:Notify({
                Title = "WHITELISTED",
                Text = "Added user to whitelist",
                Duration = 3
            })
        end
    })
    
    -- Teleport
    dupeSection:AddButton({
        Title = "Teleport to EASTER SHOP",
        Callback = function() 
            Venus:Notify({
                Title = "TELEPORTING",
                Text = "Teleporting to Easter Shop...",
                Duration = 3
            })
        end
    })
    
    -- Status
    dupeSection:AddParagraph(
        "STATUS", 
        "ALL VULN SCRIPTS ARE PATCHED AND NOW FIXING, DUPE WILL BE COMING TONIGHT LATER"
    )
    
    -- Hide all dupe features initially
    dupeSection:SetVisible(false)
end

-- Function to show DUPE features after key verification
function LoadDupeFeatures()
    if not keyVerified then return end
    
    -- Hide key elements
    keyTextBox:SetVisible(false)
    submitBtn:SetVisible(false)
    
    -- Show dupe features
    dupeSection:SetVisible(true)
end

-- Create but hide dupe features
CreateDupeFeatures()

