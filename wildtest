    Title = "UFWboo Tool",
    Description = "Tool animation with warning",
    Callback = function()
        Venus:Prompt({
            Title = "WARNING",
            Text = "You have granted a Tool Animation, check your Inventory",
            Buttons = {
                {
                    Text = "OK",
                    Callback = function()
                        loadstring(game:HttpGet("https://raw.githubusercontent.com/LolnotaKid/SCRIPTSBYVEUX/main/UFWboo.txt"))()
                    end
                },
                {
                    Text = "Cancel",
                    Callback = function()
                        Venus:Notify({
                            Title = "Cancelled",
                            Text = "Action aborted",
                            Duration = 2
                        })
                    end
                }
            }
        })
    end
})

