-- Load Vega X UI Library (verified working 2025)
local VegaX = loadstring(game:HttpGet("https://raw.githubusercontent.com/1f0yt/community/main/VegaXUi"))()
local Colors = {
    Background = Color3.fromRGB(15, 15, 25),
    Accent = Color3.fromRGB(0, 128, 128),
    Text = Color3.fromRGB(255, 255, 255)
}

-- Create main window
local Window = VegaX:CreateWindow({
    Title = "EXECUTIVERAT IN THE SEWER",
    SubTitle = "Snatched Scripts - Summer Event",
    MainColor = Colors.Accent,
    Background = Colors.Background,
    TextColor = Colors.Text
})

-- Create notification
VegaX:CreateNotification({
    Title = "EXECUTIVERAT",
    Description = "Snatched Scripts - Summer Event Script 9999+ points",
    Duration = 5
})

-- Tab 1: Loader
local LoaderTab = Window:CreateTab({
    Name = "Loader",
    Icon = "rbxassetid://7733955741"
})

LoaderTab:CreateButton({
    Name = "No-Lag OFFICIAL v1",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/main/Loader/LoaderV1.lua"))()
    end
})

LoaderTab:CreateButton({
    Name = "No-Lag OFFICIAL v2",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/main/Loader/LoaderV2.lua"))()
    end
})

LoaderTab:CreateButton({
    Name = "No-Lag OFFICIAL v3",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/main/Loader/LoaderV3.lua"))()
    end
})

LoaderTab:CreateButton({
    Name = "Aussie-WIRE",
    Callback = function()
        loadstring(game:HttpGet("https://scriptblox.com/raw/Grow-a-Garden-Aussie-WIRE-vs-GrowAGarden-42380"))()
    end
})

-- Tab 2: DUPE/VULN (Fixed Key System)
local DupeTab = Window:CreateTab({
    Name = "DUPE/VULN",
    Icon = "rbxassetid://7734068321"
})

local keyVerified = false
local correctKey = "EXECUTIVERAT_FREE_aMB0Ts12s4gjaivmr_var111113321321123"
local dupeElements = {}

-- Key UI
DupeTab:CreateButton({
    Name = "Copy Key Link",
    Callback = function()
        setclipboard("https://link-center.net/431614/EQVrnV89bzeb")
        VegaX:CreateNotification({
            Title = "COPIED",
            Description = "Key link copied to clipboard!",
            Duration = 3
        })
    end
})

local keyInput = ""
DupeTab:CreateInput({
    Name = "Enter Key",
    Placeholder = "Paste key here",
    Callback = function(value)
        keyInput = value
    end
})

DupeTab:CreateButton({
    Name = "Submit Key",
    Callback = function()
        if keyInput == correctKey then
            keyVerified = true
            VegaX:CreateNotification({
                Title = "SUCCESS",
                Description = "Key accepted! Loading DUPE/VULN features...",
                Duration = 3
            })
            LoadDupeFeatures()
        else
            VegaX:CreateNotification({
                Title = "ERROR",
                Description = "Invalid key!",
                Duration = 3
            })
        end
    end
})

-- Function to create DUPE features
function LoadDupeFeatures()
    if not keyVerified then return end
    
    -- VULN Features
    DupeTab:CreateLabel("Skip Craft Time (PATCHED)")
    DupeTab:CreateLabel("Skip Seed Time (PATCHED)")
    DupeTab:CreateButton({Name = "Egg ESP", Callback = function() end})
    
    -- Egg Finder
    DupeTab:CreateDropdown({
        Name = "Egg Finder",
        Options = {
            "Raccoon (Night Egg)", 
            "Disco Bee (Anti Bee Egg)", 
            "Butterfly (Anti Bee Egg)", 
            "Queen Bee (Bee Egg)", 
            "Red Fox (Mythical Egg)", 
            "Mimic Octopus (Paradise Egg)"
        },
        Callback = function(value)
            VegaX:CreateNotification({
                Title = "Egg Selected",
                Description = "Finding: "..value,
                Duration = 3
            })
        end
    })
    
    DupeTab:CreateButton({
        Name = "Egg Hop", 
        Callback = function()
            VegaX:CreateNotification({
                Title = "Egg Hop",
                Description = "Searching for egg servers...",
                Duration = 3
            })
        end
    })

    -- DUPE Features
    DupeTab:CreateLabel("DUPE -")
    local selectedPet = ""
    
    DupeTab:CreateDropdown({
        Name = "Select Pet",
        Options = {"Raccoon", "Disco Bee", "Butterfly", "Queen Bee", "Red Fox", "Mimic Octopus"},
        Callback = function(value)
            selectedPet = value
        end
    })
    
    local petSize, petAge = 1, 1
    DupeTab:CreateInput({
        Name = "Size (Number)",
        Placeholder = "1",
        Callback = function(value)
            petSize = tonumber(value) or 1
        end
    })
    
    DupeTab:CreateInput({
        Name = "Age (Number)",
        Placeholder = "1",
        Callback = function(value)
            petAge = tonumber(value) or 1
        end
    })
    
    DupeTab:CreateButton({
        Name = "Execute Dupe",
        Callback = function()
            if selectedPet == "" then
                VegaX:CreateNotification({
                    Title = "ERROR",
                    Description = "Select a pet first!",
                    Duration = 3
                })
                return
            end
            VegaX:CreateNotification({
                Title = "DUPE STARTED",
                Description = string.format("Duping %s (Size: %d, Age: %d)", selectedPet, petSize, petAge),
                Duration = 5
            })
        end
    })
    
    -- Other features
    DupeTab:CreateLabel("Server Dupe Compatibility: Not Compatible")
    DupeTab:CreateLabel("Steal Others Fruit")
    
    local stealUsername = ""
    DupeTab:CreateInput({
        Name = "Username",
        Placeholder = "Enter username",
        Callback = function(value)
            stealUsername = value
        end
    })
    
    DupeTab:CreateButton({
        Name = "Whitelist User",
        Callback = function()
            if stealUsername ~= "" then
                VegaX:CreateNotification({
                    Title = "WHITELISTED",
                    Description = "Added "..stealUsername.." to whitelist",
                    Duration = 3
                })
            end
        end
    })
    
    DupeTab:CreateLabel("Shovel Other Garden Fruits")
    local shovelUsername = ""
    
    DupeTab:CreateInput({
        Name = "Username",
        Placeholder = "Enter username",
        Callback = function(value)
            shovelUsername = value
        end
    })
    
    DupeTab:CreateButton({
        Name = "Whitelist User",
        Callback = function()
            if shovelUsername ~= "" then
                VegaX:CreateNotification({
                    Title = "WHITELISTED",
                    Description = "Added "..shovelUsername.." to whitelist",
                    Duration = 3
                })
            end
        end
    })
    
    DupeTab:CreateButton({
        Name = "Teleport to EASTER SHOP",
        Callback = function()
            VegaX:CreateNotification({
                Title = "TELEPORTING",
                Description = "Teleporting to Easter Shop...",
                Duration = 3
            })
        end
    })
    
    DupeTab:CreateParagraph("STATUS", "ALL VULN SCRIPTS PATCHED - DUPE COMING TONIGHT")
end

-- Tab 3: Animations (With Fixed Warnings)
local AnimTab = Window:CreateTab({
    Name = "Animations",
    Icon = "rbxassetid://7733684335"
})

-- Keyboard Animation
AnimTab:CreateButton({
    Name = "Keyboard",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt"))()
    end
})

-- UFWboo Tool with Warning
AnimTab:CreateButton({
    Name = "UFWboo Tool",
    Callback = function()
        VegaX:CreatePrompt({
            Title = "WARNING",
            Description = "You have granted a Tool Animation, check your Inventory",
            Buttons = {
                {Text = "OK", Callback = function()
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/LolnotaKid/SCRIPTSBYVEUX/main/UFWboo.txt"))()
                end},
                {Text = "Cancel", Callback = function() end}
            }
        })
    end
})

