-- Load Venyx UI Library (active and maintained)
local Venyx = loadstring(game:HttpGet("https://raw.githubusercontent.com/Stefanuk12/Venyx-UI-Library/main/source2.lua"))()
local UI = Venyx.new({
    title = "EXECUTIVERAT IN THE SEWER",
    theme = {
        Background = Color3.fromRGB(15, 15, 25),
        Accent = Color3.fromRGB(0, 128, 128)
    }
})

-- Initial notification
UI:Notify({
    title = "EXECUTIVERAT",
    text = "Snatched Scripts - Summer Event Script 9999+ points",
    duration = 5
})

-- Tab 1: Loader
local LoaderTab = UI:addPage({title = "Loader", icon = 7733955741})
LoaderTab:addButton({title = "No-Lag OFFICIAL v1", callback = function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/main/Loader/LoaderV1.lua"))()
end})

-- Add other loader buttons similarly...

-- Tab 2: DUPE/VULN (Fixed Key System)
local DupeTab = UI:addPage({title = "DUPE/VULN", icon = 7734068321})
local keyVerified = false
local correctKey = "EXECUTIVERAT_FREE_aMB0Ts12s4gjaivmr_var111113321321123"

-- Key UI
DupeTab:addButton({title = "Copy Key Link", callback = function()
    setclipboard("https://link-center.net/431614/EQVrnV89bzeb")
    UI:Notify({title = "COPIED", text = "Key link copied!", duration = 3})
end})

local keyInput = ""
DupeTab:addTextbox({title = "Enter Key", callback = function(value)
    keyInput = value
    if keyInput == correctKey then
        keyVerified = true
        UI:Notify({title = "SUCCESS", text = "Key accepted! Loading features...", duration = 3})
        LoadDupeFeatures() -- Unlock features
    else
        UI:Notify({title = "ERROR", text = "Invalid key!", duration = 3})
    end
end})

-- DUPE Features (hidden until key verified)
local dupeElements = {}
function LoadDupeFeatures()
    if not keyVerified then return end
    
    -- VULN Features
    dupeElements.eggESP = DupeTab:addButton({title = "Egg ESP", callback = function() end})
    dupeElements.eggFinder = DupeTab:addDropdown({
        title = "Egg Finder", 
        list = {"Raccoon (Night Egg)", "Disco Bee (Anti Bee Egg)", ...},
        callback = function(value)
            UI:Notify({title = "Finding", text = value, duration = 3})
        end
    })
    
    -- Add other DUPE/VULN features here...
end

-- Tab 3: Animations (With Warnings)
local AnimTab = UI:addPage({title = "Animations", icon = 7733684335})

-- Risky buttons with warnings
AnimTab:addButton({title = "UFWboo Tool", callback = function()
    UI:Notify({
        title = "WARNING", 
        text = "You granted a Tool Animation. Check your Inventory!",
        duration = 5,
        callback = function() 
            loadstring(game:HttpGet("https://raw.githubusercontent.com/LolnotaKid/SCRIPTSBYVEUX/main/UFWboo.txt"))()
        end
    })
end})

AnimTab:addButton({title = "TROLL TAB", callback = function()
    UI:Notify({
        title = "TROLL WARNING", 
        text = "This may cause unexpected effects!",
        duration = 5,
        callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/p0e1/1/main/Fe%20R6%20Animation"))()
        end
    })
end})

-- Initialize UI
UI:SelectPage(LoaderTab, true)
