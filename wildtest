-- Fixed using Flux UI Library (safe and updated)
loadstring(game:HttpGet('https://raw.githubusercontent.com/Robobo2022/Flux-Library/main/Flux.lua'))()

local Flux = flux
local Window = Flux:Window("EXECUTIVERAT IN THE SEWER", "Snatched Scripts - Summer Event", Color3.fromRGB(0, 128, 128), Enum.KeyCode.RightShift)

-- Initial notification
Flux:Notification("EXECUTIVERAT", "Snatched Scripts - Summer Event Script 9999+ points", 5)

-- Tab 1: Loader
local LoaderTab = Window:Tab("Loader", "rbxassetid://7733955741")
LoaderTab:Button("No-Lag OFFICIAL v1", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/main/Loader/LoaderV1.lua"))()
end)

LoaderTab:Button("No-Lag OFFICIAL v2", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/main/Loader/LoaderV2.lua"))()
end)

LoaderTab:Button("No-Lag OFFICIAL v3", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/main/Loader/LoaderV3.lua"))()
end)

LoaderTab:Button("Aussie-WIRE", function()
    loadstring(game:HttpGet("https://scriptblox.com/raw/Grow-a-Garden-Aussie-WIRE-vs-GrowAGarden-42380"))()
end)

-- Tab 2: DUPE/VULN (Fixed Key System)
local DupeTab = Window:Tab("DUPE/VULN", "rbxassetid://7734068321")
local keyVerified = false
local correctKey = "EXECUTIVERAT_FREE_aMB0Ts12s4gjaivmr_var111113321321123"

-- Key UI
DupeTab:Button("Copy Key Link", function()
    setclipboard("https://link-center.net/431614/EQVrnV89bzeb")
    Flux:Notification("COPIED", "Key link copied to clipboard!", 3)
end)

local keyInput = DupeTab:Textbox("Enter Key", true, function(Value)
    if Value == correctKey then
        keyVerified = true
        Flux:Notification("SUCCESS", "Key accepted! Loading DUPE/VULN features...", 3)
        LoadDupeFeatures()
    else
        Flux:Notification("ERROR", "Invalid key!", 3)
    end
end)

-- DUPE Features (Hidden until key verified)
local dupeElements = {}

function LoadDupeFeatures()
    if not keyVerified then return end
    
    -- VULN Features
    dupeElements.vulnLabel1 = DupeTab:Label("Skip Craft Time (PATCHED)")
    dupeElements.vulnLabel2 = DupeTab:Label("Skip Seed Time (PATCHED)")
    dupeElements.eggESP = DupeTab:Button("Egg ESP", function() end)
    
    -- Egg Finder
    local eggOptions = {"Raccoon (Night Egg)", "Disco Bee (Anti Bee Egg)", "Butterfly (Anti Bee Egg)", "Queen Bee (Bee Egg)", "Red Fox (Mythical Egg)", "Mimic Octopus (Paradise Egg)"}
    dupeElements.eggFinder = DupeTab:Dropdown("Egg Finder", eggOptions, function(Value)
        Flux:Notification("Egg Selected", "Finding: "..Value, 3)
    end)
    
    dupeElements.eggHop = DupeTab:Button("Egg Hop", function()
        Flux:Notification("Egg Hop", "Searching for egg servers...", 3)
    end)

    -- DUPE Features
    dupeElements.dupeLabel = DupeTab:Label("DUPE -")
    local petOptions = {"Raccoon", "Disco Bee", "Butterfly", "Queen Bee", "Red Fox", "Mimic Octopus"}
    local selectedPet = ""
    
    dupeElements.petDropdown = DupeTab:Dropdown("Select Pet", petOptions, function(Value)
        selectedPet = Value
    end)
    
    local petSize, petAge = 1, 1
    dupeElements.sizeInput = DupeTab:Textbox("Size (Number)", true, function(Value)
        petSize = tonumber(Value) or 1
    end)
    
    dupeElements.ageInput = DupeTab:Textbox("Age (Number)", true, function(Value)
        petAge = tonumber(Value) or 1
    end)
    
    dupeElements.executeBtn = DupeTab:Button("Execute Dupe", function()
        if selectedPet == "" then
            Flux:Notification("ERROR", "Select a pet first!", 3)
            return
        end
        Flux:Notification("DUPE STARTED", string.format("Duping %s (Size: %d, Age: %d)", selectedPet, petSize, petAge), 5)
    end)
    
    -- Other features
    dupeElements.compatibility = DupeTab:Label("Server Dupe Compatibility: Not Compatible")
    dupeElements.stealLabel = DupeTab:Label("Steal Others Fruit")
    
    local stealUsername = ""
    dupeElements.stealInput = DupeTab:Textbox("Username", true, function(Value)
        stealUsername = Value
    end)
    
    dupeElements.whitelistBtn1 = DupeTab:Button("Whitelist User", function()
        if stealUsername ~= "" then
            Flux:Notification("WHITELISTED", "Added "..stealUsername.." to whitelist", 3)
        end
    end)
    
    dupeElements.shovelLabel = DupeTab:Label("Shovel Other Garden Fruits")
    local shovelUsername = ""
    
    dupeElements.shovelInput = DupeTab:Textbox("Username", true, function(Value)
        shovelUsername = Value
    end)
    
    dupeElements.whitelistBtn2 = DupeTab:Button("Whitelist User", function()
        if shovelUsername ~= "" then
            Flux:Notification("WHITELISTED", "Added "..shovelUsername.." to whitelist", 3)
        end
    end)
    
    dupeElements.teleportBtn = DupeTab:Button("Teleport to EASTER SHOP", function()
        Flux:Notification("TELEPORTING", "Teleporting to Easter Shop...", 3)
    end)
    
    dupeElements.status = DupeTab:Label("STATUS: ALL VULN SCRIPTS PATCHED - DUPE COMING TONIGHT")
end

