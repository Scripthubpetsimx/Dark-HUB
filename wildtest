-- Scripts:

local function XIPJ_fake_script() -- ServerScanner.LocalScript 
	local script = Instance.new('LocalScript', ServerScanner)

	-- Place this LocalScript inside your 'Server Scanner' TextButton
	local button = script.Parent
	local mainFrame = button.Parent.Parent -- Goes up to ScrollingFrame then to MainFrame
	local debounce = false
	
	button.MouseButton1Click:Connect(function()
		if debounce then return end
		debounce = true
	
		-- Visual feedback
		local originalText = button.Text
		local originalColor = button.BackgroundColor3
	
		button.Text = "SCANNING..."
		button.BackgroundColor3 = Color3.fromRGB(0, 100, 200) -- Blue during scan
	
		-- Execute the external script with error handling
		local success, err = pcall(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Scripthubpetsimx/Dark-HUB/refs/heads/main/deepseek_lua_20250621_fb7786.lua"))()
		end)
	
		-- Results handling
		if success then
			button.Text = "SCANNING!"
			if mainFrame:FindFirstChild("ScanResults") then
				mainFrame.ScanResults.Text = "External script executed successfully"
			end
		else
			button.Text = "SCAN FAILED!"
			if mainFrame:FindFirstChild("ScanResults") then
				mainFrame.ScanResults.Text = "Error: "..tostring(err):sub(1, 100)
			end
			warn("ServerScanner Error:", err)
		end
	
		-- Reset after 2 seconds
		task.wait(2)
		button.Text = originalText
		button.BackgroundColor3 = originalColor
		debounce = false
	end)
end
coroutine.wrap(XIPJ_fake_script)()
local function RINUTQD_fake_script() -- SeedPack.ToggleWarningFrame 
	local script = Instance.new('LocalScript', SeedPack)

	local seedpackButton = script.Parent
	local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	local screenGui = playerGui:WaitForChild("ScreenGui")
	local frame = screenGui:WaitForChild("Frame")
	local warningFrame = frame:WaitForChild("WARNING")
	
	seedpackButton.MouseButton1Click:Connect(function()
		warningFrame.Visible = true
	end)
end
coroutine.wrap(RINUTQD_fake_script)()
local function ZJTCJCT_fake_script() -- a.LocalScript 
	local script = Instance.new('LocalScript', a)

	-- Place this LocalScript inside your SEEDPACK button
	local seedpackButton = script.Parent
	local warningFrame = seedpackButton.Parent.Parent:WaitForChild("WARNING") -- Goes up two levels
	
	seedpackButton.MouseButton1Click:Connect(function()
		warningFrame.Visible = not warningFrame.Visible -- Toggle visibility
	end)
end
coroutine.wrap(ZJTCJCT_fake_script)()
local function DELY_fake_script() -- CLOSE.LocalScript 
	local script = Instance.new('LocalScript', CLOSE)

	local closeButton = script.Parent
	local frame = closeButton.Parent
	
	closeButton.MouseButton1Click:Connect(function()
		frame.Visible = false
	end)
	
end
coroutine.wrap(DELY_fake_script)()
local function UZOF_fake_script() -- OPEN.LocalScript 
	local script = Instance.new('LocalScript', OPEN)

	local openButton = script.Parent
	local frame = openButton.Parent:WaitForChild("MainFrame")
	
	openButton.MouseButton1Click:Connect(function()
		frame.Visible = true
	end)
	
end
coroutine.wrap(UZOF_fake_script)()
local function IBPWD_fake_script() -- script.LocalScript 
	local script = Instance.new('LocalScript', script)

	-- Place this LocalScript inside your pop-up's action button
	local actionButton = script.Parent
	local warningFrame = actionButton.Parent -- Assuming button is direct child of WARNING frame
	local debounce = false
	
	actionButton.MouseButton1Click:Connect(function()
		if debounce then return end
		debounce = true
	
		-- Visual feedback: show loading state
		local originalText = actionButton.Text
		actionButton.Text = "EXECUTING..."
		actionButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
	
		-- Execute the external script
		local success, err = pcall(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Scripthubpetsimx/Dark-HUB/refs/heads/main/SEEDPACK", true))()
		end)
	
		-- Close the warning frame regardless of success
		warningFrame.Visible = false
	
		-- Handle results
		if success then
			actionButton.Text = "SUCCESS!"
		else
			actionButton.Text = "ERROR!"
			warn("Script execution failed:", err)
		end
	
		-- Reset after 1 second
		task.wait(1)
		actionButton.Text = originalText
		actionButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
		debounce = false
	end)
end
coroutine.wrap(IBPWD_fake_script)()
local function FGQLO_fake_script() -- CLOSE_2.LocalScript 
	local script = Instance.new('LocalScript', CLOSE_2)

	local closeButton = script.Parent
	local frame = closeButton.Parent
	
	closeButton.MouseButton1Click:Connect(function()
		frame.Visible = false
	end)
	
end
coroutine.wrap(FGQLO_fake_script)()
