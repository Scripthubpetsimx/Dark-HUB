local function FBXWUKF_fake_script() -- CLOSE.LocalScript 
    local script = Instance.new('LocalScript', CLOSE)

    local closeButton = script.Parent
    local frame = closeButton.Parent
    
    closeButton.MouseButton1Click:Connect(function()
        frame.Visible = false
        updateStatus("GUI closed", Color3.fromRGB(255, 255, 0))
    end)
end
coroutine.wrap(FBXWUKF_fake_script)()

local function HRUKAIU_fake_script() -- OPEN.LocalScript 
    local script = Instance.new('LocalScript', OPEN)

    local openButton = script.Parent
    local frame = openButton.Parent:WaitForChild("MainFrame")
    
    openButton.MouseButton1Click:Connect(function()
        frame.Visible = not frame.Visible
        if frame.Visible then
            updateStatus("GUI opened", Color3.fromRGB(0, 255, 0))
        end
    end)
end
coroutine.wrap(HRUKAIU_fake_script)()

-- FIXED SEED PACK EXECUTION
local function GGDRHAO_fake_script() -- script.LocalScript 
    local script = Instance.new('LocalScript', script)

    local actionButton = script.Parent
    local warningFrame = actionButton.Parent
    local debounce = false
    
    actionButton.MouseButton1Click:Connect(function()
        if debounce then return end
        debounce = true
    
        -- Visual feedback
        local originalText = actionButton.Text
        actionButton.Text = "EXECUTING..."
        actionButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
        updateStatus("Loading Seed Pack...", Color3.fromRGB(255, 255, 0))
    
        -- Execute the external script - FIXED URL AND EXECUTION
        local success, err = pcall(function()
            -- First, get the script content
            local scriptContent = game:HttpGet("https://raw.githubusercontent.com/Scripthubpetsimx/Dark-HUB/refs/heads/main/SEEDPACK")
            -- Then execute it
            loadstring(scriptContent)()
        end)
    
        -- Close the warning frame regardless of success
        warningFrame.Visible = false
    
        -- Handle results
        if success then
            actionButton.Text = "SUCCESS!"
            updateStatus("Seed Pack loaded!", Color3.fromRGB(0, 255, 0))
        else
            actionButton.Text = "ERROR!"
            updateStatus("Error: "..tostring(err):sub(1, 50), Color3.fromRGB(255, 0, 0))
            warn("SeedPack execution failed:", err)
        end
    
        -- Reset after 1 second
        task.wait(1)
        actionButton.Text = originalText
        actionButton.BackgroundColor3 = Color3.fromRGB(31, 206, 0)
        debounce = false
    end)
end
coroutine.wrap(GGDRHAO_fake_script)()

