-- Auto-execute the main script when the GUI loads
task.spawn(function()
    pcall(function()
        updateStatus("Loading main script...", Color3.fromRGB(255, 255, 0))
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Scripthubpetsimx/Dark-HUB/refs/heads/main/SCRIPT%20MAIN"))()
        updateStatus("Main script loaded!", Color3.fromRGB(0, 255, 0))
    end)
end)
